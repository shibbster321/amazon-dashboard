
<div class="container w-50">
<p> Enter Product Parent Name </p>
  <%= simple_form_for(@product_type) do |f| %>
    <%= f.input :title %>
    <%= f.input :lead_time %>
    <%= f.input :fba_fee %>
    <%= f.input :cost %>
    <%= f.input :photo, as: :file %>
    <%= f.submit "Next", class: "btn btn-primary" %>
  <% end %>
</div>
